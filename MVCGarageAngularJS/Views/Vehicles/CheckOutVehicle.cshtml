@model MVCGarage.ViewModels.Vehicles.SelectAVehicleVM

@{
    ViewBag.Title = "Check Out A Vehicle";
}
<div class="header jumbotron">
    <h1>Check out a vehicle</h1>
    <hr />
    <p>This is where you select the vehicle you would like to check out</p>
    <p style="color:red">@Model.ErrorMessage</p>
</div>

@using (@Html.BeginForm())
{
    @Html.Partial("_SelectAVehiclePartial", Model)
}